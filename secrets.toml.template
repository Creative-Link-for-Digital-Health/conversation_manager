# API Secrets Configuration
# Copy this file to .secrets.toml and fill in your actual API keys
# Add .secrets.toml to your .gitignore file!

[OPENAI]
API_KEY = "sk-your-openai-api-key-here"
API_URL = "https://api.openai.com/v1"
MODEL = "gpt-4"

[OPENROUTER]
API_KEY = "sk-or-your-openrouter-key-here"
API_URL = "https://openrouter.ai/api/v1"
MODEL = "anthropic/claude-3-sonnet"

[LLM_FACTORY]
API_KEY = "your-llm-factory-key-here"
API_URL = "https://your-llm-factory-endpoint.com/v1"
MODEL = "custom-model"

[REDCAP]
API_TOKEN = "your-redcap-api-token-here"
API_URL = "https://redcap.your-institution.edu/api/"
PROJECT_ID = "12345"